{
  "extends": "@tsconfig/node18/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "downlevelIteration": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "strictNullChecks": true
  },
  "exclude": [
    "auto-api",
    "es",
    "lib",
    "node_modules"
  ]
}